<% content_for :title do %>Dashboard<% end %>
<%= link_to "New blog", new_post_path %>
<%= link_to "Home", posts_path %>
<br>
<% @posts.each do |post| %>
	<%= link_to post.title, post %><br>
	<%= link_to "Edit", edit_post_path(post) %>
	<%= link_to "Delete", post, :confirm=>"Are you sure?", :method=>:delete %>
<hr>
<% end %>