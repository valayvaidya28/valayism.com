<% content_for :title do %>Edit post<% end %>
<h1> Edit post </h1>

<%= form_for @post, {} ,:html => {:multipart => true} do |f| %>
<p>
	<%= f.label :title %><br />
	<%= f.text_field :title %>
</p>
<p>
	<%= f.label :content %><br />
	<%= f.text_area :content %>
</p>
<p>
	<%= f.file_field :photo %>
</p>
<p>
	<%= f.submit "Update" %>
</p>
<% end %>